        <Button fx:id="Endurance" layoutX="102.0" layoutY="174.0" mnemonicParsing="false" onAction="#handleEndurance" prefHeight="25.0" prefWidth="109.0" text="Endurance" />
        <Button fx:id="flexibility" layoutX="102.0" layoutY="237.0" mnemonicParsing="false" onAction="#handleFlexibility" prefHeight="25.0" prefWidth="109.0" text="Flexibility" />
