                <Font size="15.0" />
            </font>
        </TitledPane>
        <Button fx:id="Aerobic" layoutX="102.0" layoutY="60.0" mnemonicParsing="false" onAction="#handleAerobic" prefHeight="25.0" prefWidth="109.0" text="Aerobic" />
