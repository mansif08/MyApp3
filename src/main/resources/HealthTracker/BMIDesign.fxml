<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="358.0" prefWidth="307.0" stylesheets="@DecorateHealth.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="HealthTracker.BMIController">
    <children>
        <TitledPane animated="false" layoutY="-5.0" prefHeight="25.0" prefWidth="309.0" text="Count Your BMI" textFill="#a64747">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
            <font>
                <Font size="16.0" />
            </font>
        </TitledPane>
        <TextField fx:id="BMI" layoutX="19.0" layoutY="179.0" prefHeight="26.0" prefWidth="178.0" text="Your BMI :" />
        <TextField fx:id="HealthyWeight" layoutX="19.0" layoutY="231.0" prefHeight="26.0" prefWidth="236.0" text="Your Healthy Weight :" />
        <TextField fx:id="Suggestion" layoutX="19.0" layoutY="281.0" prefHeight="26.0" prefWidth="284.0" text="Suggestion:" />
      <TextField fx:id="Weight" layoutX="59.0" layoutY="47.0" promptText="Add Weight in Kilogram" />
      <TextField fx:id="Height" layoutX="59.0" layoutY="88.0" promptText="Add Height in Inches" />
      <Button fx:id="Submit" layoutX="93.0" layoutY="129.0" mnemonicParsing="false" onAction="#HandleSubmit" text="Submit" />
      <Label fx:id="validInput" layoutX="169.0" layoutY="133.0" prefHeight="18.0" prefWidth="120.0" textFill="#d92727" />
    </children>
</AnchorPane>
